<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Developer" table="Developer">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="image" type="string" column="image" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="string" column="surname" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="personalInformation" type="string" column="personal_information" length="512" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="linkedin" type="string" column="linkedin" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="twitter" type="string" column="twitter" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="github" type="string" column="github" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
